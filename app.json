    "android": {
      "package": "com.africanstore.africanstore",
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#48B600"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "78afb50a-848e-4b3b-80f4-50570c2f167b"
