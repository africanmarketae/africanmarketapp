{
  "expo": {
    "name": "africanstore",
    "slug": "africanstore-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "userInterfaceStyle": "light",
    "scheme": "africanstore-app",
    "splash": {
      "image": "./assets/unsplash.png",
